# BTC Elite Trader - Environment Variables
# Copy this to .env and fill in your values

# Telegram Bot (get from @BotFather)
TELEGRAM_BOT_TOKEN=your_bot_token_here
TELEGRAM_CHAT_ID=your_chat_id_here

# Binance API (create at binance.com/en/my/settings/api-management)
# IMPORTANT: Disable withdrawal permissions!
BINANCE_API_KEY=your_api_key_here
BINANCE_API_SECRET=your_api_secret_here

# Database (PostgreSQL)
BTC_DB_PASSWORD=your_db_password_here

# Optional: For testnet
BINANCE_TESTNET_API_KEY=your_testnet_key_here
BINANCE_TESTNET_API_SECRET=your_testnet_secret_here
